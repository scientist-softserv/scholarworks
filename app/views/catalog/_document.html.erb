<%
  # OVERRIDE file from hyrax v3.6.0
  # Customization: Customize search results to have thumbnails, model type, title & short description
%>

<li id="document_<%= document.to_param %>" class="document <%= render_document_class document %>" itemscope itemtype="<%= document.itemtype %>">
  <div class="row search-result-wrapper">
    <div class="col-md-2">
      <div class="list-thumbnail">
        <%= render_thumbnail_tag document %>
      </div>
    </div>
    <div class="col-md-10">
      <%= render 'csu/result', document: document %>
    </div>
  </div>
</li>
